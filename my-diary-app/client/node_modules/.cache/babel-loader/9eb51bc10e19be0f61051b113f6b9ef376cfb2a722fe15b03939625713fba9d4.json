{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport router from '../router'; // 라우터 인스턴스\n\nexport default {\n  data() {\n    return {\n      email: '',\n      password: '',\n      errorMessage: '' // 로그인 실패 시 에러 메시지를 저장할 변수\n    };\n  },\n  methods: {\n    login() {\n      const userData = {\n        email: this.email,\n        password: this.password\n      };\n      axios.post('http://localhost:3000/userlogin', userData).then(response => {\n        // 로그인 성공\n        alert(\"My Diary에 오신 것을 환영합니다.\");\n        console.log('로그인 성공:', response.data);\n        localStorage.setItem('userId', response.data.userId);\n        router.push('/mainpage');\n      }).catch(error => {\n        // 로그인 실패\n        if (error.response && error.response.status === 401) {\n          alert(\"잘못된 정보입니다.\\n다시 확인해주세요.\");\n        } else {\n          alert(\"알 수 없는 오류가 발생했습니다.\");\n        }\n        console.error('로그인 실패:', error.response);\n        this.errorMessage = error.response.data.message || \"로그인에 실패했습니다.\";\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","router","data","email","password","errorMessage","methods","login","userData","post","then","response","alert","console","log","localStorage","setItem","userId","push","catch","error","status","message"],"sources":["C:\\Users\\ewoo2\\OneDrive\\문서\\GitHub\\공개SW프로젝트-Win\\Internet_Programming\\my-diary-app\\client\\src\\components\\UserLogin.vue"],"sourcesContent":["<template>\r\n  <div class=\"login-container\">\r\n    <div class=\"login-box\">\r\n      <h1 class=\"login-header\">My Diary</h1>\r\n      <div class=\"inputs-container\">\r\n        <div class=\"fields-container\">\r\n          <input type=\"email\" placeholder=\"이메일\" class=\"input-field\" v-model=\"email\" @keyup.enter=\"login\" />\r\n          <input type=\"password\" placeholder=\"비밀번호\" class=\"input-field\" v-model=\"password\" @keyup.enter=\"login\" />\r\n        </div>\r\n        <button class=\"login-btn\" @click=\"login\">로그인</button>\r\n      </div>\r\n      <div class=\"links\">\r\n        <a href=\"/userregister\">회원가입</a> |\r\n        <a href=\"/find-account\">아이디/비밀번호 찾기</a>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  import axios from 'axios';\r\n  import router from '../router'; // 라우터 인스턴스\r\n\r\n  export default {\r\n    data() {\r\n        return {\r\n        email: '',\r\n        password: '',\r\n        errorMessage: '' // 로그인 실패 시 에러 메시지를 저장할 변수\r\n        };\r\n    },\r\n    methods: {\r\n      login() {\r\n      const userData = {\r\n        email: this.email,\r\n        password: this.password\r\n      };\r\n      \r\n      axios.post('http://localhost:3000/userlogin', userData)\r\n        .then(response => {\r\n          // 로그인 성공\r\n          alert(\"My Diary에 오신 것을 환영합니다.\");\r\n          console.log('로그인 성공:', response.data);\r\n          localStorage.setItem('userId', response.data.userId);\r\n          router.push('/mainpage');\r\n        })\r\n        .catch(error => {\r\n          // 로그인 실패\r\n          if (error.response && error.response.status === 401) {\r\n            alert(\"잘못된 정보입니다.\\n다시 확인해주세요.\");\r\n          } else {\r\n            alert(\"알 수 없는 오류가 발생했습니다.\");\r\n          }\r\n          console.error('로그인 실패:', error.response);\r\n          this.errorMessage = error.response.data.message || \"로그인에 실패했습니다.\";\r\n        });\r\n      }\r\n    }\r\n  };\r\n</script>\r\n  \r\n<style scoped>\r\n.login-container {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  height: 80vh;\r\n}\r\n\r\n.login-box {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  justify-content: center;\r\n  padding: 10px 20px 20px;\r\n  border: 2px solid #ddd;\r\n  border-radius: 8px;\r\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n  background-color: #f9f9f9;\r\n  margin: auto;\r\n  width: 300px;\r\n  height: 20vh;\r\n}\r\n\r\n.inputs-container {\r\n  display: flex;\r\n  justify-content: space-between;\r\n}\r\n\r\n.fields-container {\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.login-header {\r\n  font-size: 2em;\r\n  font-weight: bold;\r\n  margin-bottom: 20px;\r\n  margin-top: 0;\r\n}\r\n\r\n.input-field {\r\n  margin-bottom: 10px;\r\n  width: 150px;\r\n}\r\n\r\n.login-btn {\r\n  height: 52px;\r\n  width: 52px;\r\n  margin-left: 20px;\r\n  background-color: blue;\r\n  color: white;\r\n  cursor: pointer;\r\n  border: none;\r\n}\r\n\r\n.links {\r\n  text-align: center;\r\n  width: 100%;\r\n  padding-top: 10px;\r\n}\r\n</style>\r\n"],"mappings":";AAoBE,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,MAAK,MAAO,WAAW,EAAE;;AAEhC,eAAe;EACbC,IAAIA,CAAA,EAAG;IACH,OAAO;MACPC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,YAAY,EAAE,EAAC,CAAE;IACjB,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACPC,KAAKA,CAAA,EAAG;MACR,MAAMC,QAAO,GAAI;QACfL,KAAK,EAAE,IAAI,CAACA,KAAK;QACjBC,QAAQ,EAAE,IAAI,CAACA;MACjB,CAAC;MAEDJ,KAAK,CAACS,IAAI,CAAC,iCAAiC,EAAED,QAAQ,EACnDE,IAAI,CAACC,QAAO,IAAK;QAChB;QACAC,KAAK,CAAC,wBAAwB,CAAC;QAC/BC,OAAO,CAACC,GAAG,CAAC,SAAS,EAAEH,QAAQ,CAACT,IAAI,CAAC;QACrCa,YAAY,CAACC,OAAO,CAAC,QAAQ,EAAEL,QAAQ,CAACT,IAAI,CAACe,MAAM,CAAC;QACpDhB,MAAM,CAACiB,IAAI,CAAC,WAAW,CAAC;MAC1B,CAAC,EACAC,KAAK,CAACC,KAAI,IAAK;QACd;QACA,IAAIA,KAAK,CAACT,QAAO,IAAKS,KAAK,CAACT,QAAQ,CAACU,MAAK,KAAM,GAAG,EAAE;UACnDT,KAAK,CAAC,wBAAwB,CAAC;QACjC,OAAO;UACLA,KAAK,CAAC,oBAAoB,CAAC;QAC7B;QACAC,OAAO,CAACO,KAAK,CAAC,SAAS,EAAEA,KAAK,CAACT,QAAQ,CAAC;QACxC,IAAI,CAACN,YAAW,GAAIe,KAAK,CAACT,QAAQ,CAACT,IAAI,CAACoB,OAAM,IAAK,cAAc;MACnE,CAAC,CAAC;IACJ;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}